   "cell_type": "code",
   "execution_count": 227,
   "id": "914fa8b8-1e6a-45ea-80b0-bf35d3ebcc08",
   "metadata": {},
   "outputs": [],
   "source": [
    "import pandas as pd\n",
    "\n",
    "def clean_columns(df, rename_map=None):\n",
    "    \"\"\"\n",
    "    Cleans DataFrame column names:\n",
    "    - Converts to lowercase\n",
    "    - Replaces spaces & special characters with underscores\n",
    "    - Strips leading/trailing underscores\n",
    "    - Optionally renames specific columns using a dictionary\n",
    "    \"\"\"\n",
    "    df.columns = (\n",
    "        df.columns\n",
    "        .str.lower()\n",
    "        .str.replace(r'\\W+', '_', regex=True)\n",
    "        .str.strip('_')\n",
    "    )\n",
    "    \n",
    "    if rename_map:\n",
    "        df = df.rename(columns=rename_map)\n",
    "    \n",
    "    return df\n"
   ]
  },
